  "\[IndentingNewLine]", 
  RowBox[{"52160", "*", "49.5"}], "-", "\[IndentingNewLine]", 
  RowBox[{"2555863.", "*", 
   RowBox[{"(", 
    RowBox[{"1", "+", 
     RowBox[{"0.05", "/", "52"}]}], ")"}]}]}]], "Input",
